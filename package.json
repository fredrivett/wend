{
  "name": "syncdocs",
  "version": "0.1.2",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "syncdocs": "./dist/index.mjs"
  },
  "scripts": {
    "dev": "bash scripts/run.sh",
    "dev:cli": "tsdown src/cli/index.ts --watch",
    "build": "tsdown src/cli/index.ts && npm run build:viewer",
    "build:viewer": "vite build --config src/server/viewer/vite.config.ts",
    "syncdocs": "node dist/index.mjs",
    "test": "vitest",
    "test:extractor": "tsx scripts/test-extractor.ts",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "format:unsafe": "biome check --write --unsafe ."
  },
  "keywords": [],
  "author": "@fredrivett",
  "license": "ISC",
  "description": "Docs that automatically sync with your code",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@changesets/cli": "^2.29.8",
    "@tailwindcss/postcss": "^4.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/marked": "^5.0.2",
    "@types/picomatch": "^4.0.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@xyflow/react": "^12.10.0",
    "clsx": "^2.1.1",
    "elkjs": "^0.11.0",
    "lucide-react": "^0.574.0",
    "marked": "^17.0.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.20.1",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "usehooks-ts": "^3.1.1",
    "vaul": "^1.1.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "cac": "^6.7.14",
    "picomatch": "^4.0.3",
    "radix-ui": "^1.4.3",
    "typescript": "^5.9.3"
  }
}
